{
    "name": "my-network",
    "version": "1.0.0",
    "client": {
      "organization": "Org1",
      "connection": {
        "timeout": {
          "peer": {
            "endorse": "300"
          }
        }
      }
    },
    "organizations": {
      "Org1": {
        "mspid": "Org1MSP",
        "peers": [
          "peer0.org1.example.com"
        ],
        "certificate": "path/to/your/cert.pem"
      }
    },
    "peers": {
      "peer0.org1.example.com": {
        "url": "grpcs://localhost:7051",
        "tlsCACerts": {
          "path": "path/to/your/tls/ca.crt"
        }
      }
    }
  }
  